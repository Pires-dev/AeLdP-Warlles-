programa {
  funcao inicio() {

 // Declaração de Variaáveis.
   real altura, peso_ideal

// Declaração de Constantes.
  const real K=72.7
  const inteiro B=58

// Entrada de Dados.
  escreva (" Informe a sua altura:")
  leia (altura)

// Processamento.
  peso_ideal = (K * altura) - B

// Saída de Dados.
  escreva ("Seu peso ideal, de acordo com sua altura, é:" , peso_ideal) 

  }

}
