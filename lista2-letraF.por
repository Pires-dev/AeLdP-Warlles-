programa {
  funcao inicio() {
// Delcaração de Variáveis.
  real V , R , A
// Entrada de Dados.
  escreva (" Informe o valor do Raio: ")
  leia (R)
  escreva (" Agora, da Altura: ")
  leia (A)
// Processamento.
  V = 3.14 * R * A
// Saída de Dados.
  escreva (" Temos então, que o Volume é de: " , V )

  }
}
